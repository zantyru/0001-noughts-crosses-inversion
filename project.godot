; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Noughts, crosses and inversion"
run/main_scene="res://scenes2/Main.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://scenes2/_global/Global.tscn"

[debug]

settings/crash_handler/message="Crash..."

[display]

window/size/resizable=false
window/vsync/use_vsync=false
window/energy_saving/keep_screen_on=false

[editor]

active=false

[locale]

translations=PoolStringArray( "res://spoilers/translations/translation.en.translation", "res://spoilers/translations/translation.ru.translation" )
locale_filter=[ 0, [  ] ]

[physics]

common/physics_fps=12
common/enable_object_picking=false

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
quality/2d/use_pixel_snap=true
quality/filters/anisotropic_filter_level=1
quality/shadows/filter_mode=0
quality/depth_prepass/enable=false
quality/subsurface_scattering/quality=0
quality/subsurface_scattering/weight_samples=false
quality/voxel_cone_tracing/high_quality=false
environment/default_clear_color=Color( 1, 1, 1, 1 )
quality/depth/hdr=false
